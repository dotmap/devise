{
  "parser": "babel-eslint",
  "extends": "vue",
  "plugins": ["html", "flowtype-errors", "disable"],
  "rules": {
    "import/no-unresolved": 0,
    "import/no-unassigned-import": 0,
    "flowtype-errors/show-errors": 2
  },
  "settings": {
    "eslint-plugin-disable": {
      "paths": {
        "*": ["*"],
        "flowtype-errors": ["**/*.js"]
      }
    }
  }
}