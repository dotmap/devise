{
  "name": "vuepack-typescript",
  "version": "1.0.0",
  "description": "Modification of egoist's vuepack template with Typescript instead of ES6.",
  "main": "client/index.js",
  "scripts": {
    "test": "jest",
    "vorlon": "vorlon",
    "build": "webpack --config build/webpack.prod.js",
    "build:types": "vuetype client",
    "watch:types": "vuetype client -w",
    "dev": "node build/server.js",
    "lint": "tslint ./**/*.ts -p ./tsconfig.json -t stylish"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "author": "Graham Barber <grahammbarber@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "promise-polyfill": "^6.0.2",
    "typescript": "^2.3.4",
    "vue": "^2.2.0",
    "vue-router": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-class": "^0.2.0",
    "vuex-router-sync": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "autoprefixer": "^6.4.0",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "^1.5.1",
    "babel-runtime": "^6.18.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint-config-vue": "latest",
    "eslint-plugin-vue": "latest",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-plugin-remove": "^0.0.3",
    "jest": "^20.0.4",
    "offline-plugin": "^4.5.3",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "style-loader": "^0.13.1",
    "ts-jest": "^20.0.5",
    "ts-loader": "^2.1.0",
    "typescript-eslint-parser": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "vorlon": "^0.5.4",
    "vue-class-component": "^5.0.1",
    "vue-loader": "^11.0.0",
    "vue-property-decorator": "^5.0.1",
    "vue-template-compiler": "^2.2.0",
    "vuetype": "^0.2.2",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
