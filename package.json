{
  "name": "devise",
  "version": "1.0.0",
  "description": "Making great game documentation.",
  "author": "Graham Barber <grahammbarber@gmail.com>",
  "private": true,
  "scripts": {
    "build": "run-s build:**",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:autoprefixer": "postcss --use autoprefixer -o public/assets/css/app.css public/assets/css/app.css",
    "build:cssnano": "cssnano public/assets/css/app.css public/assets/css/app.css",
    "dev": "cross-env NODE_ENV=development rollup -cw",
    "test:eslint": "eslint src/* --ext .vue --ext .js --fix; exit 0"
  },
  "dependencies": {
    "bulma": "^0.4.1",
    "quill": "^1.2.4",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "cross-env": "^5.0.0",
    "cssnano-cli": "^1.0.4",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.0.1",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^4.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-vue": "^2.3.3",
    "rollup-watch": "^3.2.2"
  }
