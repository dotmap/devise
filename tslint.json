{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "no-shadowed-variable": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ]
    },
    "rulesDirectory": []
}